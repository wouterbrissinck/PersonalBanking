<Window x:Class="Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:presentation="clr-namespace:Wpf"
        Title="MainWindow" Height="800" Width="1000" Margin="10">
    <Window.Resources>
    </Window.Resources>
    <Grid>
        <TabControl Name="m_MainTabs" >
            <TabItem Header="Import" Name="m_TabImport">
                <presentation:ImportTab></presentation:ImportTab>
            </TabItem>
            <TabItem Header="Transactions" Name="m_TabTransactions">
                <presentation:TransactionsTab></presentation:TransactionsTab>
            </TabItem>
            <TabItem Header="Categories" Name="m_TabCategorize">
                <presentation:CategoriesTab></presentation:CategoriesTab>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
